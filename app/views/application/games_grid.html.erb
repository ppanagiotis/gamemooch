<div class="page-header">
        <h1><%= @title %></h1>
</div
<div align="center">
        <%= form_tag(@action, :method => "get") do %>
                <%= select_tag "selectImage", options_for_select(games_grid(@games)), multiple: true, class: 'image-picker show-labels show-html'%>
                <div align="center">
                        <%= submit_tag @submit_tag, class: "btn btn-primary btn-lg btn-block" %>
                </div>
        <% end %>
</div>
<div align="center">
        <%= will_paginate @games %>
</div>
